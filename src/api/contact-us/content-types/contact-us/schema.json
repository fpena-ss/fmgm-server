{
  "kind": "singleType",
  "collectionName": "contact_uses",
  "info": {
    "singularName": "contact-us",
    "pluralName": "contact-uses",
    "displayName": "Contact Us"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "preview": {
      "enabled": true
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "body": {
      "type": "blocks",
      "required": true
    },
    "contactInfo": {
      "type": "component",
      "component": "forms.contact-info",
      "repeatable": false,
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    }
  }
}
